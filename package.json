{
  "name": "seoscript",
  "version": "0.1.1",
  "description": "A comprehensive SEO analysis tool for modern web applications",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "seoscript": "./dist/cli/index.js"
  },
  "scripts": {
    "start": "tsx src/cli/index.ts",
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "keywords": [
    "seo",
    "analysis",
    "typescript",
    "cli",
    "web",
    "react",
    "vue",
    "angular"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^22.8.2",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@types/node": "^20.9.1",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  }
}
